{
    "manifest_version": 2,
    "app": {
        "background": {
            "scripts": ["launch.js"]
        }
    },
    "description": "mespeech Chrome packaged app.",
    "icons": {
        "16": "images/msp-16.png",
        "128": "images/msp-128.png",
        "256": "images/msp-256.png"
