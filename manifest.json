{
    "name": "mespeech URL",
    "version": "1.0.7.5",
    "manifest_version": 2,
    "app": {
        "launch": {
            "local_path": "pages/index/index.html"        
        }
    },
    "description": "With the url Permission.",
    "icons": {
        "16": "images/msp-16.png",
        "128": "images/msp-128.png",
        "256": "images/msp-256.png"
    },
    "minimum_chrome_version": "14",
    "permissions": [ 
        "tts",
        "unlimitedStorage",
        "storage",
        "https://mespee.ch/*",
        "http://mespee.ch/*"
    ]
}
